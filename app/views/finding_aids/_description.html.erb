<h3><%= @scope_content_head %></h3>
<% @scope_content_values.each do |scope_content_value| %>
  <%== apply_ead_to_html_transforms scope_content_value %>
<% end %>
<p>
  <ul style="list-style-type:none;">
    <% @series_titles.each_with_index do |series_title, index| %>
    <li>
      <p> <%= link_to series_title,
	      # slug_to_series_page.call(ser_id: index + 1)
	      repository_finding_aid_component_path(id: index+1, finding_aid_id: params[:id])
	      %> </p>
      <p> <%= @series_scope_content[index] %> </p>
    </li>
    <% end %>
  </ul>
</p>
<%= content_tag(:h3,@odd_head) unless @odd_head.nil? %>
<% @odd_values.each do |odd_value| %>
  <%== apply_ead_to_html_transforms odd_value %>
<% end %>
